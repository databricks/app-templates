{
  "name": "@chat-template/ai-sdk-providers",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.cjs"
        }
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/node": "^22.8.6"
  },
  "dependencies": {
    "@chat-template/utils": "*",
    "@chat-template/auth": "*",
    "ai": "5.0.76",
    "zod": "^3.25.76"
  }
}
